r\n            Designs\r\n          </Typography>\r\n          {auth && (\r\n            <div>\r\n              <Button\r\n              text=\"Add New\"\r\n              color=\"inherit\"\r\n              startIcon={<AddBox />}\r\n              className ={classes.newButton}\r\n              onClick={() => setOpenPopup(true)}/>\r\n            </div>\r\n          )}\r\n        </Toolbar>\r\n      </AppBar>\r\n      <Popup\r\n        title = \"Add Design\"\r\n        openPopup={openPopup}\r\n        setOpenPopup={setOpenPopup}\r\n        >\r\n          <ArtistForm\r\n          addOrEdit={addOrEdit}/>\r\n      </Popup>\r\n    </div>\r\n  );\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c9f406c5ed5d3d02f03a58c7a20b12a7","cacheData":{"env":{}}}